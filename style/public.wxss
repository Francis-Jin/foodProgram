/** 1px边框. */

.border_top {
    position: relative;
}

.border_top:before {
    content: "";
    position: absolute;
    transform: scaleY(0.5);
    transform-origin: 0 0;
    top: -.5px;
    left: 0;
    right: 0;
    border-top: 1px solid var(--border_color);
}

.border_bottom {
    position: relative;
}

.border_bottom:after {
    content: "";
    position: absolute;
    transform: scaleY(0.5);
    transform-origin: 0 0;
    bottom: 0;
    left: 0;
    right: 0;
    border-bottom: 1rpx solid var(--border_color);
}

.border_left {
    position: relative;
}

.border_left:before {
    content: "";
    position: absolute;
    transform: scaleX(0.5);
    transform-origin: 0 0;
    top: 0;
    left: -.5px;
    bottom: 0;
    border-left: 1px solid var(--border_color);
}

.border_right {
    position: relative;
}

.border_right:after {
    content: "";
    position: absolute;
    transform: scaleX(0.5);
    transform-origin: 0 0;
    top: 0;
    right: 1rpx;
    bottom: 0;
    border-right: 1rpx solid var(--border_color);
}

.border_all {
    position: relative;
}

.border_all:before {
    content: "";
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    border: 1rpx solid var(--border_color);
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    box-sizing: border-box;
    border-radius: 0px;
}

